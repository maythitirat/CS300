public class Taxi {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		ArrayList<String> time = new ArrayList<>();
		for (int i = 0; i < n*2; i++) {
			time.add(scn.nextInt());
		}
		
		

	}

}
