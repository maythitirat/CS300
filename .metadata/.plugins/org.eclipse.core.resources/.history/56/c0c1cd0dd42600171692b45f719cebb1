import java.util.ArrayList;
import java.util.Scanner;

public class PeriodChecker {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		int m = scn.nextInt();

		int[][] man = new int[n][m];
		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {
				man[j][i] = scn.nextInt();
			}
		}
		int sum = 0;
		for (int i = 0; i < n; i++) {
			for (int j = m - 1; j > 0; j-) {
				sum = man[i][j] - man[i][j-1];
				System.out.println(sum);
			}
		}
	}

}
