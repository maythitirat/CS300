import java.util.ArrayList;
import java.util.Scanner;

public class EqualizeList {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int n = scn.nextInt();
		ArrayList<Integer> a = new ArrayList<>();
		for (int i = 0; i < n; i++) {
			a.add(scn.nextInt());
		}
	}
}
